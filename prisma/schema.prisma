generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PG_URL")
}

model User {
  id          Int      @id @default(autoincrement())
  name        String   @unique
  email       String   @unique
  password    String
  isSuperUser Boolean  @default(false)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
