{
  "name": "webapp-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd next dev",
    "build": "next build",
    "start": "next start",
    "check:types": "tsc -p .",
    "dc:up": "env-cmd sh -c \"cd dev; docker-compose up\"",
    "dc:destroy": "env-cmd sh -c \"cd dev; docker-compose down; docker-compose rm -f\""
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn check:types"
    }
  },
  "dependencies": {
    "next": "10.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "env-cmd": "^10.1.0",
    "husky": "^4.3.6",
    "typescript": "^4.1.3"
  }
}
